{
  "name": "anonymize-tool",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.10.0"
  },
  "resolutions": {
    "cross-spawn": ">=6.0.6",
    "dompurify": ">=3.2.4",
    "esbuild": ">=0.25.0",
    "estree-util-value-to-estree": ">=3.3.3"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --fix --max-warnings=9999",
    "lint:fix": "node scripts/eslint-fix-advanced.js",
    "fix:types": "node scripts/fix-typescript-errors.js",
    "unified-fix": "node scripts/unified-fix.js",
    "sync-aliases": "node scripts/sync-aliases.js",
    "sync-aliases:fix": "node scripts/sync-aliases.js --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:staged": "jest --findRelatedTests",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "generate": "plop",
    "gen:component": "plop",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "playwright test e2e/a11y.spec.ts",
    "test:components-a11y": "playwright test e2e/components-a11y.spec.ts",
    "test:bundle-size": "ANALYZE=true next build",
    "analyze": "node scripts/run-analyzer.js",
    "type-check": "tsc --noEmit",
    "type-check:full": "tsc --noEmit --skipLibCheck false --strict",
    "check-imports": "node scripts/fix-imports.js",
    "fix-imports": "node scripts/fix-imports.js --fix",
    "check-styles": "node scripts/check-styles.js",
    "check-web-vitals": "node scripts/check-web-vitals.js",
    "check-security": "pnpm audit",
    "update-imports": "node scripts/update-imports.js",
    "apply-memo": "node scripts/apply-memo.js",
    "apply-memo:fix": "node scripts/apply-memo.js --apply",
    "check-paths": "node scripts/check-paths.js",
    "update-paths": "node scripts/update-paths.js",
    "validate-paths": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@hookform/resolvers": "3.10.0",
    "@radix-ui/react-slot": "1.2.3",
    "@sentry/nextjs": "9.24.0",
    "@types/dompurify": "3.0.5",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dompurify": "3.2.6",
    "framer-motion": "^12.9.1",
    "jest-environment-jsdom": "29.7.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lucide-react": "0.511.0",
    "next": "14.2.29",
    "next-intl": "3.26.5",
    "next-themes": "0.4.6",
    "nextra": "2.13.4",
    "nextra-theme-docs": "2.13.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.6.0",
    "ts-node": "10.9.2",
    "web-vitals": "4.2.4",
    "zustand": "4.5.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.1",
    "@axe-core/react": "4.10.1",
    "@chromatic-com/storybook": "1.9.0",
    "@eslint/eslintrc": "3.3.1",
    "@formatjs/intl": "^3.1.6",
    "@heroicons/react": "^2.2.0",
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "14.2.29",
