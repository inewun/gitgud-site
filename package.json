    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "glob": "^11.0.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-axe": "10.0.0",
    "lint-staged": "15.5.2",
    "minimist": "1.2.8",
    "next-auth": "^4.24.11",
    "plop": "4.0.1",
    "prettier": "3.5.3",
    "react-hook-form": "7.57.0",
    "react-redux": "^9.2.0",
    "semantic-release": "23.1.1",
    "storybook": "8.6.14",
    "superjson": "^2.2.2",
    "tailwindcss": "3.4.17",
