{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/public/*": ["./public/*"],
      "@/test/*": ["./src/__tests__/*"],
      "@/e2e/*": ["./e2e/*"],
      "@/app/*": ["./src/app/*"],
      "@/app/components/*": ["./src/app/components/*"],
      "@/app/api/*": ["./src/app/api/*"],
      "@/app/docs/*": ["./src/app/docs/*"],
      "@/app/_layers/*": ["./src/app/_layers/*"],
      "@/app/_layers/ui/*": ["./src/app/_layers/ui/*"],
      "@/app/tool/*": ["./src/app/tool/*"],
      "@/app/anonymize/*": ["./src/app/anonymize/*"],
      "@/widgets/*": ["./src/widgets/*"],
      "@/features/*": ["./src/features/*"],
      "@/entities/*": ["./src/entities/*"],
      "@/shared/*": ["./src/shared/*"],
      "@/shared/ui/*": ["./src/shared/ui/*"],
      "@/shared/ui/molecules/*": ["./src/shared/ui/molecules/*"],
      "@/shared/ui/organisms/*": ["./src/shared/ui/organisms/*"],
      "@/shared/ui/theme/*": ["./src/shared/ui/theme/*"],
      "@/shared/ui/client/*": ["./src/shared/ui/client/*"],
      "@/shared/ui/accessibility/*": ["./src/shared/ui/accessibility/*"],
      "@/shared/ui/navigation/*": ["./src/shared/ui/navigation/*"],
      "@/shared/ui/card/*": ["./src/shared/ui/card/*"],
      "@/shared/ui/virtualization/*": ["./src/shared/ui/virtualization/*"],
      "@/shared/ui/hydration/*": ["./src/shared/ui/hydration/*"],
      "@/shared/ui/server/*": ["./src/shared/ui/server/*"],
      "@/shared/ui/performance/*": ["./src/shared/ui/performance/*"],
      "@/shared/ui/media/*": ["./src/shared/ui/media/*"],
      "@/shared/ui/animations/*": ["./src/shared/ui/animations/*"],
      "@/shared/ui/button/*": ["./src/shared/ui/button/*"],
      "@/shared/ui/feedback/*": ["./src/shared/ui/feedback/*"],
      "@/shared/ui/layout/*": ["./src/shared/ui/layout/*"],
      "@/shared/ui/inputs/*": ["./src/shared/ui/inputs/*"],
      "@/shared/ui/providers/*": ["./src/shared/ui/providers/*"],
      "@/shared/ui/preload/*": ["./src/shared/ui/preload/*"],
      "@/shared/api/*": ["./src/shared/api/*"],
      "@/shared/lib/*": ["./src/shared/lib/*"],
      "@/shared/hooks/*": ["./src/shared/hooks/*"],
      "@/shared/providers/*": ["./src/shared/providers/*"],
      "@/shared/stories/*": ["./src/shared/stories/*"],
      "@/domain/*": ["./src/domain/*"],
      "@/styles/*": ["./src/styles/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/services/*": ["./src/services/*"],
      "@/components/*": ["./src/components/*"],
      "@/molecules/*": ["./src/shared/ui/molecules/*"],
      "@/types/*": ["./src/types/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/store/*": ["./src/store/*"],
      "@/scripts/*": ["./src/scripts/*"],
      "@/locales/*": ["./src/locales/*"],
      "@/docs/*": ["./src/docs/*"],
      "@/middleware/*": ["./src/middleware/*"],
      "@/tailwindcss/*": ["./node_modules/tailwindcss/*"],
      "@/storybook/*": ["./node_modules/@storybook/*"],
      "@/testing-library/*": ["./node_modules/@testing-library/*"],
      "@/playwright/*": ["./node_modules/@playwright/*"],
      "@/axe-core/*": ["./node_modules/@axe-core/*"],
      "@/heroicons/*": ["./node_modules/@heroicons/*"],
      "@/formatjs/*": ["./node_modules/@formatjs/*"],
      "@/trpc/*": ["./node_modules/@trpc/*"],
      "@/jest/*": ["./node_modules/@jest/*"],
      "@/sentry/*": ["./node_modules/@sentry/*"],
      "@/hookform/*": ["./node_modules/@hookform/*"],
      "@/reduxjs/*": ["./node_modules/@reduxjs/*"],
      "@/next-auth/*": ["./node_modules/next-auth/*"],
      "@/zod/*": ["./node_modules/zod/*"],
      "@/framer-motion/*": ["./node_modules/framer-motion/*"],
      "@/lucide-react/*": ["./node_modules/lucide-react/*"],
      "@/next-intl/*": ["./node_modules/next-intl/*"],
      "@/nextra/*": ["./node_modules/nextra/*"],
      "@/zustand/*": ["./node_modules/zustand/*"],
      "@/radix-ui/*": ["./node_modules/@radix-ui/*"],
      "@/lodash/*": ["./node_modules/lodash/*"],
      "@/lodash-es/*": ["./node_modules/lodash-es/*"],
      "@/superjson/*": ["./node_modules/superjson/*"]
    },
    "baseUrl": "."
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "e2e/**/*.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "scripts/**/*.js",
    "scripts/**/*.ts",
    "src/app/**/*.ts",
    "src/app/**/*.tsx",
    "src/widgets/**/*.ts",
    "src/widgets/**/*.tsx",
    "src/features/**/*.ts",
    "src/features/**/*.tsx",
    "src/entities/**/*.ts",
    "src/entities/**/*.tsx",
    "src/shared/**/*.ts",
    "src/shared/**/*.tsx",
    "src/shared/ui/**/*.ts",
    "src/shared/ui/**/*.tsx",
    "src/shared/api/**/*.ts",
    "src/shared/api/**/*.tsx",
    "src/shared/lib/**/*.ts",
    "src/shared/lib/**/*.tsx",
    "src/shared/hooks/**/*.ts",
    "src/shared/hooks/**/*.tsx",
    "src/shared/providers/**/*.ts",
    "src/shared/providers/**/*.tsx",
    "src/shared/stories/**/*.ts",
    "src/shared/stories/**/*.tsx",
    "src/domain/**/*.ts",
    "src/domain/**/*.tsx",
    "src/hooks/**/*.ts",
    "src/hooks/**/*.tsx",
    "src/lib/**/*.ts",
    "src/lib/**/*.tsx",
    "src/scripts/**/*.ts",
    "src/scripts/**/*.js",
    "src/types/**/*.ts",
    "src/types/**/*.tsx",
    "src/services/**/*.ts",
    "src/services/**/*.tsx",
    "src/store/**/*.ts",
    "src/store/**/*.tsx",
    "src/locales/**/*.ts",
    "src/locales/**/*.tsx",
    "src/docs/**/*.ts",
    "src/docs/**/*.tsx",
    "src/__tests__/**/*.ts",
    "src/__tests__/**/*.tsx",
    "__tests__/**/*.ts",
    "__tests__/**/*.tsx",
    "public/**/*.ts",
    "public/**/*.tsx"
  ],
  "exclude": ["node_modules"]
}
